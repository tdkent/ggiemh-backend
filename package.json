{
	"name": "ggiemh-backend",
	"private": true,
	"version": "1.0.0",
	"description": "REST API server for the Model Homes of the Golden Gate International Exposition website.",
	"main": "server.js",
	"scripts": {
		"build": "tsc && tsc-alias",
		"dev": "tsx watch src/server.ts",
		"lint": "biome check --verbose",
		"lint:fix": "biome check --write --no-errors-on-unmatched",
		"prepare": "husky",
		"start": "node dist/server.js",
		"test": "vitest"
	},
	"lint-staged": {
		"*": "biome check --write --no-errors-on-unmatched --verbose"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tdkent/ggiemh-backend.git"
	},
	"author": "Tim Kent <timkentdev@gmail.com>",
	"type": "module",
	"homepage": "https://github.com/tdkent/ggiemh-backend#readme",
	"devDependencies": {
		"@biomejs/biome": "2.3.12",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.6",
		"@types/node": "^25.0.10",
		"@types/supertest": "^6.0.3",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"supertest": "^7.2.2",
		"tsc-alias": "^1.8.16",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite-tsconfig-paths": "^6.0.5",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"cors": "^2.8.6",
		"dotenv": "^17.2.3",
		"express": "^5.2.1",
		"mongodb": "^7.0.0",
		"winston": "^3.19.0"
	}
}
